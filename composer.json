{
    "name": "gerrymcdonnell/changelog",
    "description": "Document current tasks within your project and prioritise them.",
    "type": "cakephp-plugin",
	"keywords": ["log","logging","changelog","cakephp","plugin"],
    "homepage": "http://www.irishbloke.net/?tag=changelogs",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Gerrymcdonnell\\Changelog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gerrymcdonnell\\Changelog\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
